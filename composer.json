{
  "name": "bayfrontmedia/bones-app",
  "description": "A simple boilerplate to begin building an application using the Bones framework.",
  "homepage": "https://github.com/bayfrontmedia/bones-app",
  "license" : "MIT",
  "type": "project",
  "keywords": [
    "bones",
    "bones-app"
  ],
  "authors": [
    {
      "name": "John Robinson",
      "email": "john@bayfrontmedia.com",
      "homepage": "https://www.bayfrontmedia.com"
    }
  ],
  "support": {
    "issues": "https://github.com/bayfrontmedia/bones-app/issues"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist"
  },
  "require": {
    "php": "^8.0",
    "bayfrontmedia/bones": "^3.1"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    },
    "classmap": [
      "resources/database/migrations/"
    ]
  }
}
